{
  "name": "pomo",
  "version": "0.2.3",
  "description": "A simple pomodoro timer with 3rd party integrations",
  "main": "./build/main.js",
  "scripts": {
    "clean": "git clean -dfx && yarn install",
    "dev": "FORCE_COLOR=1 run-p --print-label dev:electron dev:react",
    "machine": "xstate typegen \"client/machines/**/machine.ts\"",
    "storybook": "start-storybook -p 6006 -s public -c tooling/storybook",
    "pretty:check": "prettier --check --ignore-path .gitignore .",
    "pretty": "prettier --write --ignore-path .gitignore .",
    "lint": "eslint .",
    "test": "jest",
    "visual": "backstop test --config=\"tooling/backstop.config.js\" --docker",
    "e2e": "INTEGRATION=true playwright test",
    "docker-e2e": "docker build . -f Dockerfile -t pancake",
    "build": "FORCE_COLOR=1 run-p --print-label build:electron build:react",
    "start": "electron .",
    "build:electron": "webpack --config tooling/webpack/electron.webpack.js --mode production",
    "build:react": "webpack --config tooling/webpack/react.webpack.js --mode production",
    "dev:electron": "wait-on http-get://localhost:4000/ && cross-env NODE_ENV=development webpack --config tooling/webpack/electron.webpack.js --mode development && yarn start",
    "dev:react": "cross-env NODE_ENV=development webpack-dev-server --config tooling/webpack/react.webpack.js --mode development",
    "checks": "FORCE_COLOR=1 npm-run-all --print-label --parallel lint test --serial build",
    "release": "electron-builder --mac",
    "postrelease": "rm -rf dist",
    "postinstall": "electron-builder install-app-deps"
  },
  "keywords": [],
  "author": "Adam Hall <adamhalldesigns@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.13.8",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.13.8",
    "@babel/plugin-proposal-optional-chaining": "7.13.8",
    "@babel/preset-env": "7.13.9",
    "@babel/preset-react": "7.12.13",
    "@babel/preset-typescript": "7.13.0",
    "@playwright/test": "^1.20.2",
    "@storybook/addon-actions": "6.4.19",
    "@storybook/addon-essentials": "6.4.19",
    "@storybook/addon-links": "6.4.19",
    "@storybook/addons": "6.4.19",
    "@storybook/react": "6.4.19",
    "@storybook/theming": "6.4.19",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "12.1.4",
    "@testing-library/react-hooks": "7.0.2",
    "@testing-library/user-event": "13.5.0",
    "@types/electron-devtools-installer": "2.2.0",
    "@types/jest": "26.0.20",
    "@types/lodash.merge": "4.6.6",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.1",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "5.12.0",
    "@typescript-eslint/parser": "5.12.0",
    "babel-loader": "8.2.2",
    "babel-plugin-styled-components": "1.13.2",
    "backstopjs": "6.0.4",
    "copy-webpack-plugin": "6",
    "cross-env": "7.0.3",
    "electron-builder": "22.7.0",
    "electron-builder-notarize": "1.2.0",
    "eslint": "7",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "16.1.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest-dom": "3.6.5",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "0.5.7",
    "eslint-plugin-testing-library": "^5.2.1",
    "html-webpack-plugin": "4.5.2",
    "jest": "26.6.3",
    "npm-run-all": "4.1.5",
    "playwright": "^1.20.2",
    "prettier": "2.2.1",
    "spectron": "13.0.0",
    "ts-jest": "26.5.2",
    "typescript": "4.5.5",
    "wait-on": "5.2.1",
    "webpack": "4.46.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.2"
  },
  "dependencies": {
    "@xstate/cli": "0.1.5",
    "@xstate/inspect": "0.6.2",
    "@xstate/react": "2.0.0",
    "dotenv": "10.0.0",
    "electron": "16.0.10",
    "electron-devtools-installer": "3.1.1",
    "electron-log": "4.3.2",
    "electron-store": "8.0.0",
    "electron-updater": "4.3.5",
    "got": "11.8.2",
    "lodash.merge": "4.6.2",
    "menubar": "9.1.1",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "styled-components": "5.2.1",
    "ws": "8.5.0",
    "xstate": "4.30.1"
  },
  "build": {
    "appId": "com.electron.pomo",
    "productName": "Pomo",
    "afterSign": "electron-builder-notarize",
    "linux": {
      "category": "Utility"
    },
    "mac": {
      "category": "public.app-category.developer-tools",
      "entitlements": "./publishingTools/entitlements.mac.plist",
      "entitlementsInherit": "./publishingTools/entitlements.mac.plist",
      "hardenedRuntime": true
    },
    "directories": {
      "output": "dist",
      "buildResources": "publishingTools"
    },
    "files": [
      "package.json",
      "publishingTools/icon.*",
      "build/**"
    ]
  },
  "volta": {
    "node": "16.9.1",
    "yarn": "1.22.18"
  },
  "packageManager": "yarn@3.2.0"
}
